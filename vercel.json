{
  "version": 2,
  "env": {},
  "functions": {
    "api/*.ru": {
      "runtime": "@vercel/ruby@1.3.16"
    }
  },
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/week/:year/:num/:wday", "destination": "/api/week" },
    { "source": "/api/week/:year/:num/:wday(.:format)?", "destination": "/api/week" }
  ]
}
